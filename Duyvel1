if npc and npc:FindFirstChild("HumanoidRootPart") then
    -- ƒê·∫∑t v·ªã tr√≠ c√°ch NPC 0.5 studs l√™n tr√™n
    hrp.CFrame = npc.HumanoidRootPart.CFrame * CFrame.new(0, npc.HumanoidRootPart.Size.Y/2 + 0.5, 0)

    -- Gi·ªØ kh√¥ng r∆°i xu·ªëng
    local bv = Instance.new("BodyVelocity")
    bv.Velocity = Vector3.new(0, 0, 0)
    bv.MaxForce = Vector3.new(1e9, 1e9, 1e9)
    bv.Parent = hrp
end

end)
local drops = serv:Channel("B√ö ü§ë‚≠ê")
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

local selectedPlayer = nil

-- H√†m l·∫•y danh s√°ch t√™n player
local function GetPlayerNames()
    local names = {}
    for _, plr in ipairs(Players:GetPlayers()) do
        if plr ~= LocalPlayer then
            table.insert(names, plr.Name)
        end
    end
    return names
end

-- Dropdown
drops:Dropdown(
    "Ch·ªçn ng∆∞·ªùi ch∆°i",
    GetPlayerNames(),
    function(value)
        selectedPlayer = value
        print("ƒê√£ ch·ªçn:", selectedPlayer)
    end
)

-- N√∫t Gay
drops:Button("L·∫•y skill c·ªßa ng∆∞·ªùi ƒë√≥", function()
    if not selectedPlayer then
        warn("B·∫°n ch∆∞a ch·ªçn ng∆∞·ªùi ch∆°i")
        return
    end

    local target = Players:FindFirstChild(selectedPlayer)
    if not target then
        warn("Kh√¥ng t√¨m th·∫•y player:", selectedPlayer)
        return
    end

    -- Ch·ªâ k√≠ch ho·∫°t RemoteEvent/BindableEvent "Fire" c·ªßa player ƒë√≥
    for _, obj in ipairs(target:GetDescendants()) do
        if obj:IsA("BindableEvent") or obj:IsA("RemoteEvent") then
            if obj.Name == "Fire" then
                if obj:IsA("BindableEvent") then
                    pcall(function()
                        obj:Fire()
                    end)
                elseif obj:IsA("RemoteEvent") then
                    pcall(function()
                        obj:FireServer()
                    end)
                end
            end
        end
    end

    print("ƒê√£ k√≠ch ho·∫°t c√°c 'Fire' Event c·ªßa:", selectedPlayer)
end)

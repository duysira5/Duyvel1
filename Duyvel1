-- G·ª¨I WEBHOOK
local function sendWebhook(p, whitelisted)
    local dateNow = os.date("%Y-%m-%d")     -- Ng√†y hi·ªán t·∫°i
    local timeNow = os.date("%H:%M:%S")     -- Gi·ªù hi·ªán t·∫°i

    local isWhitelisted = whitelist[p.Name]

    local titleText = whitelisted and "‚úÖ Player Are Whitelisted VN Piece" or "‚ö†Ô∏è Player Not Whitelisted VN Piece"
    local noteText = whitelisted and "Player is on the whitelist" or "Not whitelisted"
    local colorCode = whitelisted and 65280 or 16711680  -- Xanh n·∫øu c√≥ whitelist, ƒë·ªè n·∫øu kh√¥ng

    local jsonData = HttpService:JSONEncode({
        username = "Fumo Fumo",
        embeds = {{
            title = titleText,
            description = "**Name:** " .. p.Name ..
                         "\n**UserId:** " .. tostring(p.UserId) ..
                         "\n**Date:** " .. dateNow ..
                         "\n**Time:** " .. timeNow ..
                         "\n**Note:** " .. noteText,
            color = colorCode
        }}
    })

    local request = http_request or request or (syn and syn.request) or (fluxus and fluxus.request)
    if request then
        request({
            Url = "https://discord.com/api/webhooks/1402869800421691402/U-hGPDXbLYyOrhyD7aISvor_eYXfO90TjTMBHS3uis8VHuDoO6W2_o2fEqv6sv8FfOxe",
            Method = "POST",
            Headers = {["Content-Type"] = "application/json"},
            Body = jsonData
        })
    else
        warn("Executor kh√¥ng h·ªó tr·ª£ HTTP Request. Kh√¥ng th·ªÉ g·ª≠i webhook.")
    end
end

-- KI·ªÇM TRA WHITELIST
if not whitelist[player.Name] then
    sendWebhook(player, false)
    task.wait(1)
    player:Kick("i see you")
    return
else
    sendWebhook(player, true)
end

